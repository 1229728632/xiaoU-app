
.homeHeader {
    width: 100vw;
    height: 0.52rem;
    padding: 0.1rem 0rem 0rem 0.1rem;
    box-sizing: border-box;

    .logo {
        display: inline-block;
        width: 100px;
        height: 0.25rem;
        margin-right: 0.7rem;
        box-sizing: border-box;

        img {
            width: 100%;
            height: 100%;
            vertical-align: middle;
        }
    }

    .seatch {
        display: inline-block;
        width: 0.8rem;
        height: 0.25rem;
        border-radius: 0.2rem;
        background: #ccc;
        text-align: center;
        line-height: 0.25rem;
    }
}